{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "BpmnEditor",
  "version": "0.1.0",
  "identifier": "com.tamusroyce.bpmn-editor",
  "build": {
    "beforeDevCommand": "vite",
    "beforeBuildCommand": "vite build",
    "devUrl": "http://localhost:15000",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "BPMN Editor",
        "width": 1200,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all"
  }
}
